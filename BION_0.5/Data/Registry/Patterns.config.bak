# Bamh1 aptamers,

<bamh1>
    name = bamh1_fwd
    title = bamh1 (F)
    abbrev = fwd
    patstr = CACTATA[2,0,0] GGGGCCACCAACGACATT[3,1,1] ( 34...36 GTT | 33...37 0...3 ) 0...1000$
    elems = 6
    fetch = 2
    check = 2
</bamh1>

<bamh1>
    name = bamh1_rev
    title = bamh1 (R)
    abbrev = rev
    patstr = GATCCATGGGCAC[2,1,1] TATTTATATCAAC[3,1,1] ( 34...36 AAT | 33...37 0...3 ) 0...1000$
    elems = 6
    fetch = 2
    check = 2
</bamh1>

# Poly-A,

<polya>
    name = poly_A
    title = Poly-A motif
    abbrev = polyA
    patstr = AAAAAAAAAAAAAAAAA[1,0,0]
</polya>

# Poly-C,

<polyc>
    name = poly_C
    title = Poly-C motif
    abbrev = polyC
    patstr = CCCCCCCCCCCCCCCCC[1,0,0]
</polyc>

# Poly-G,

<polyg>
    name = poly_G
    title = Poly-G motif
    abbrev = polyG
    patstr = GGGGGGGGGGGGGGGGG[1,0,0]
</polyg>

# Poly-T,

<polyt>
    name = poly_T
    title = Poly-T motif
    abbrev = polyT
    patstr = TTTTTTTTTTTTTTTTT[1,0,0]
</polyt>

